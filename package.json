{
  "name": "each-package",
  "version": "0.10.94",
  "description": "Run commands in each package folder starting with cwd skipping node_modules folders",
  "keywords": [
    "each",
    "package",
    "npm",
    "install",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/each-package.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.cjs",
  "types": "dist/cjs/index.d.cts",
  "bin": {
    "ep": "./bin/cli.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^2.1.25",
    "exit": "^0.1.2",
    "fs-iterator": "^5.2.27",
    "getopts-compat": "^2.2.6",
    "lodash.find": "^4.6.0",
    "queue-cb": "^1.4.17",
    "remove-bom-buffer": "^3.0.0",
    "spawn-streaming": "^1.0.33",
    "spawn-term": "^0.1.52",
    "topological-sort-group": "^0.1.12"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*",
    "cr": "*",
    "is-version": "*",
    "pinkie-promise": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
