{
  "name": "each-package",
  "version": "0.2.0",
  "description": "Run commands in each package folder starting with cwd skipping node_modules folders",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/each-package.git"
  },
  "keywords": [
    "each",
    "package",
    "npm",
    "install",
    "test"
  ],
  "bin": {
    "ep": "./bin/each-package"
  },
  "license": "MIT",
  "scripts": {
    "test": "mocha test/spec/**/*.test.js",
    "compat": "nvs 14,12,10,8,6,4,0.12,0.10,0.8 mocha-compat test/spec/**/*.test.js",
    "lint": "eslint .",
    "format": "prettier --write '**/*.js'"
  },
  "dependencies": {
    "commander": "^2.20.3",
    "cross-spawn-cb": "^0.1.1",
    "fs-iterator": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "mocha": "^7.1.2",
    "mocha-compat": "^3.5.5",
    "nvs": "^0.1.8",
    "prettier": "^2.0.5"
  }
}
