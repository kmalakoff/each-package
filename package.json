{
  "name": "each-package",
  "version": "0.7.0",
  "description": "Run commands in each package folder starting with cwd skipping node_modules folders",
  "keywords": [
    "each",
    "package",
    "npm",
    "install",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/each-package.git"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "ep": "./bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "call-once-fn": "^1.0.1",
    "core-js": "^3.39.0",
    "cross-spawn-cb": "^1.0.0",
    "exit": "^0.1.2",
    "foreman": "^3.0.1",
    "fs-iterator": "^5.1.0",
    "getopts-compat": "^2.2.5",
    "lodash.throttle": "^4.1.1",
    "queue-cb": "^1.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.9.3",
    "cr": "^0.1.0",
    "is-version": "^0.2.1",
    "ts-dev-stack": "^0.16.0"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs"
    ]
  }
}
